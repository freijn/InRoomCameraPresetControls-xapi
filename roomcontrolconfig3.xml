    <Page>
      <Name>Focus</Name>
      <Row>
        <Name>AutoFocus both   On/Off</Name>
        <Widget>
          <WidgetId>Autofocus</WidgetId>
          <Type>ToggleButton</Type>
          <Options>size=1</Options>
        </Widget>
        <Widget>
          <WidgetId>widget_21</WidgetId>
          <Type>Spacer</Type>
          <Options>size=3</Options>
        </Widget>
      </Row>
      <Row>
        <Name>Focus Step Speed</Name>
        <Widget>
          <WidgetId>fspeed</WidgetId>
          <Type>GroupButton</Type>
          <Options>size=4;columns=4</Options>
          <ValueSpace>
            <Value>
              <Key>100</Key>
              <Name>100x</Name>
            </Value>
            <Value>
              <Key>10</Key>
              <Name>10x</Name>
            </Value>
            <Value>
              <Key>1</Key>
              <Name>1x</Name>
            </Value>
          </ValueSpace>
        </Widget>
        <Widget>
          <WidgetId>widget_14</WidgetId>
