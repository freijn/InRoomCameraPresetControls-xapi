              <Key>9</Key>
              <Name>2B</Name>
            </Value>
            <Value>
              <Key>10</Key>
              <Name>3B</Name>
            </Value>
            <Value>
              <Key>11</Key>
              <Name>1C</Name>
            </Value>
            <Value>
              <Key>12</Key>
              <Name>2C</Name>
            </Value>
            <Value>
              <Key>13</Key>
              <Name>3C</Name>
            </Value>
          </ValueSpace>
        </Widget>
      </Row>
      <Row>
        <Name>Cam view Position</Name>
        <Widget>
          <WidgetId>NESW</WidgetId>
          <Name>:-)</Name>
          <Type>DirectionalPad</Type>
          <Options>size=4</Options>
        </Widget>
      </Row>
      <Row>
        <Name>Zoom</Name>
        <Widget>
          <WidgetId>Zoom</WidgetId>
          <Type>Spinner</Type>
          <Options>size=2;style=plusminus</Options>
        </Widget>
        <Widget>
          <WidgetId>eggtext</WidgetId>
          <Name/>
          <Type>Text</Type>
          <Options>size=1;fontSize=normal;align=center</Options>
        </Widget>
      </Row>
      <PageId>CamPresets</PageId>
      <Options/>
