<Extensions>
  <Version>1.7</Version>
  <Panel>
    <Order>2</Order>
    <PanelId>CameraPresets</PanelId>
    <Origin>local</Origin>
    <Type>Statusbar</Type>
    <Icon>Camera</Icon>
    <Color>#FF503C</Color>
    <Name>Camera Presets</Name>
    <ActivityType>Custom</ActivityType>
    <Page>
      <Name>&gt; Camera&lt;</Name>
      <Row>
        <Name>Active Camera Selection</Name>
        <Widget>
          <WidgetId>CamSelect</WidgetId>
          <Type>GroupButton</Type>
          <Options>size=4</Options>
          <ValueSpace>
            <Value>
              <Key>Cam1</Key>
              <Name>Camera 1</Name>
            </Value>
            <Value>
              <Key>Cam2</Key>
              <Name>Camera 2</Name>
            </Value>
            <Value>
              <Key>Cam3</Key>
              <Name>Camera 3</Name>
            </Value>
          </ValueSpace>
        </Widget>
      </Row>
      <Row>
        <Name>Presets  LongPress=Save</Name>
        <Widget>
          <WidgetId>Preset</WidgetId>
          <Type>GroupButton</Type>
          <Options>size=4;columns=3</Options>
          <ValueSpace>
            <Value>
              <Key>5</Key>
              <Name>1A</Name>
            </Value>
            <Value>
              <Key>6</Key>
              <Name>2A</Name>
            </Value>
