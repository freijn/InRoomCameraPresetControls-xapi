        <Widget>
          <WidgetId>wbtext</WidgetId>
          <Name>Text</Name>
          <Type>Text</Type>
          <Options>size=1;fontSize=normal;align=center</Options>
        </Widget>
      </Row>
      <PageId>Whitebalance</PageId>
      <Options>hideRowNames=0</Options>
    </Page>
    <Page>
      <Name>Settings</Name>
      <Row>
        <Name>Selfview</Name>
        <Widget>
          <WidgetId>SelfView</WidgetId>
          <Type>ToggleButton</Type>
          <Options>size=1</Options>
        </Widget>
        <Widget>
          <WidgetId>widget_26</WidgetId>
          <Type>Spacer</Type>
          <Options>size=3</Options>
        </Widget>
      </Row>
      <Row>
        <Name>Auto Standby</Name>
        <Widget>
          <WidgetId>standby</WidgetId>
          <Type>ToggleButton</Type>
