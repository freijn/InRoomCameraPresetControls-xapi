        <Widget>
          <WidgetId>Whitebalance</WidgetId>
          <Type>ToggleButton</Type>
          <Options>size=1</Options>
        </Widget>
        <Widget>
          <WidgetId>widget_20</WidgetId>
          <Type>Spacer</Type>
          <Options>size=3</Options>
        </Widget>
      </Row>
      <Row>
        <Name>White Balance</Name>
        <Widget>
          <WidgetId>wbset</WidgetId>
          <Type>Slider</Type>
          <Options>size=3</Options>
