          <Type>Text</Type>
          <Options>size=1;fontSize=normal;align=center</Options>
        </Widget>
      </Row>
      <PageId>Brightness</PageId>
      <Options/>
    </Page>
    <Page>
      <Name>WB</Name>
      <Row>
